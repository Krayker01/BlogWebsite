<%- include('partials/header'); -%>
    <h1>Day 1</h1>
    <p><%= homeStartingContent %></p>

    <% posts.forEach(function(post){ %>
      <h1> <%= post.newTitle %> </h1>
      <% if (post.newText.length>100){ %>
        <p>
          <%= post.newText.slice(0, 100) + " ..." %> <a href="/posts/<%= post.newTitle %>" >Read More</a>
        </p>
      <% } else { %>
        <p> <%= post.newText %> <p>
      <% } %>
    <% }) %>

<%- include('partials/footer'); -%>
